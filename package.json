{
  "name": "BaconServer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon --exec ./node_modules/.bin/ts-node --files ./src/index.ts",
    "test": "NODE_ENV=test jest --coverage --runInBand",
    "docker": "tsc && docker build -t bacon_server . && docker tag bacon_server:latest jeffyoun/bacon_server:latest && docker push jeffyoun/bacon_server:latest",
    "container": "tsc && docker stop bacon_server  && docker rm bacon_server   && docker create --name bacon_server -p 8001:8001 bacon_server:latest && docker start bacon_server",
    "publishAndLocal": "tsc && docker build -t bacon_server . && docker tag bacon_server:latest jeffyoun/bacon_server:latest && docker push jeffyoun/bacon_server:latest && docker stop bacon_server  && docker rm bacon_server   && docker create --name bacon_server -p 8001:8001 bacon_server:latest && docker start bacon_server"
  },
  "dependencies": {
    "aws-sdk": "^2.725.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "faker": "^4.1.0",
    "joi": "^17.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.24",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "sinon": "^9.0.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cheerio": "^0.22.21",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.4",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.32",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.3",
    "@types/multer-s3": "^2.7.7",
    "@types/node": "^14.0.14",
    "@types/sinon": "^9.0.4",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.0.0",
    "jest": "^26.1.0",
    "jest-extended": "^0.11.5",
    "mongodb-memory-server": "^6.6.3",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  }
}
